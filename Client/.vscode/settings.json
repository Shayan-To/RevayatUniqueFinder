{
  "workbench.colorCustomizations": {
    "statusBar.background": "#35b50e",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#43e412",
    "titleBar.activeBackground": "#35b50e",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#35b50e99",
    "titleBar.inactiveForeground": "#e7e7e799",
    "activityBar.activeBackground": "#43e412",
    "activityBar.activeBorder": "#4e74f1",
    "activityBar.background": "#43e412",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#4e74f1",
    "activityBarBadge.foreground": "#e7e7e7",
    "sash.hoverBorder": "#43e412",
    "statusBarItem.remoteBackground": "#35b50e",
    "statusBarItem.remoteForeground": "#e7e7e7"
  },
  "peacock.color": "#35b50e",
  "window.title": "${dirty}${activeEditorShort}${separator}RevayatUniqueFinder${separator}Client${separator}${appName}",
  "search.exclude": {
    "**/.yarn": true,
    "**/.pnp.*": true
  },
//   "prettier.prettierPath": ".yarn/sdks/prettier/index.js",
//   "typescript.tsdk": ".yarn/sdks/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
//   "eslint.nodePath": ".yarn/sdks",
  "explorer.experimental.fileNesting.enabled": true,
  "explorer.experimental.fileNesting.expand": true,
  "explorer.experimental.fileNesting.patterns": {
    "*.ts": "$(capture).js, $(capture).d.ts",
    "*.js": "$(capture).js.map, $(capture).min.js, $(capture).d.ts",
    "*.jsx": "$(capture).js",
    "*.tsx": "$(capture).ts, $(capture).scss",
    "tsconfig.json": "tsconfig.*.json",
    ".eslintrc.js": ".eslintignore",
    ".prettierrc.json": ".prettierignore, .prettier-ignore",
    "package.json": "yarn.lock, .yarnrc.yml"
  },
  "files.exclude": {
    "node_modules/": true,
    ".parcel-cache/": true,
    "dist/": true,
    ".pnp.*": true
  },
  "files.associations": {
    "yarn.lock": "yaml",
    ".parcelrc": "jsonc"
  },
  "cSpell.words": [
    "lcontent",
    "litems",
    "lself",
    "Revayat",
    "xcontent",
    "xitems",
    "xself"
  ]
}
